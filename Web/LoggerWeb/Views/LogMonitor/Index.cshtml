@model WcfLoggerService.WcfLogData

<div>
    Source: @Model.DataSourceType
</div>
<div>
    <table>
        <thead>
            <tr>
                <th>Time</th>
                <th>Size</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Logs)
            {
                <tr>
                    <td>@item.CurrentTime</td>
                    <td></td>
                </tr>
            }
        </tbody>
    </table>
</div>

